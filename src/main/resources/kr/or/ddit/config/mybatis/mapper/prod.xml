<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="prod">

	<select id="prodList" resultType="prodvo" parameterType="String" >
		select *from
	    prod
	    where
	    prod_lgu = 'P201'
	</select>



</mapper>